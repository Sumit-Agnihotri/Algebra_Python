   "source": [
    "# Concept\n",
    "Use expand() to open up powers like (2x + 1)^2.\n",
    "This is a shortcut to multiplying (2x + 1) with itself."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "82eb446d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Expanded Power: 4*x**2 + 4*x + 1\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, expand\n",
    "\n",
    "x = symbols('x')\n",
    "expr = (2*x + 1)**2\n",
    "expanded = expand(expr)\n",
    "\n",
    "print(\"Expanded Power:\", expanded)"
   ]
  }
 ],
