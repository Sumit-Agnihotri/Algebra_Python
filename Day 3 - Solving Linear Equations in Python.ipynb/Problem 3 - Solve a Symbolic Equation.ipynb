   "source": [
    "# Concept\n",
    "Let a and b be variables. The solution will be expressed symbolically."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "10216a03",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "General Solution for x: [-b/a]\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, Eq, solve\n",
    "\n",
    "a, b, x = symbols('a b x')\n",
    "eq3 = Eq(a*x + b, 0)\n",
    "solution3 = solve(eq3, x)\n",
    "\n",
    "print(\"General Solution for x:\", solution3)"
   ]
  }
