   "source": [
    "# Concept\n",
    "Use linsolve() to solve multiple equations together."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "20c73780",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Solution (x, y): {(8/3, 2/3)}\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, Eq, linsolve\n",
    "\n",
    "x = symbols('x')\n",
    "y = symbols('y')\n",
    "eq1 = Eq(2*x + y, 6)\n",
    "eq2 = Eq(x - y, 2)\n",
    "\n",
    "solution4 = linsolve((eq1, eq2), x, y)\n",
    "print(\"Solution (x, y):\", solution4)"
   ]
  }
 ],
