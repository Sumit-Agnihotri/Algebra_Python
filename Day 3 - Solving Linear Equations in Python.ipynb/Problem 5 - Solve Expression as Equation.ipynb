   "source": [
    "# Concept\n",
    "You can also solve expressions like x**2 - 4 = 0."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6992f84e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Roots of x² - 4 = 0: [-2, 2]\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, solve\n",
    "\n",
    "x = symbols('x')\n",
    "expr = x**2 - 4\n",
    "solution5 = solve(expr, x)\n",
    "\n",
    "print(\"Roots of x² - 4 = 0:\", solution5)"
   ]
  }
