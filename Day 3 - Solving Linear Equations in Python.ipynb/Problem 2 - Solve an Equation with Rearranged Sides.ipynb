    "from sympy import symbols, Eq, solve\n",
    "\n",
    "x = symbols('x')\n",
    "eq2 = Eq(5, x + 1)\n",
    "solution2 = solve(eq2, x)\n",
    "display(eq2)\n",
    "print(\"Solution:\", solution2)"
