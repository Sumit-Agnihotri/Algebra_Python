   "source": [
    "# Concept\n",
    "Handle powers and nesting like (x + 1)**2."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "61ebc003",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original: (x + 1)**2\n",
      "Expanded: x**2 + 2*x + 1\n"
     ]
    }
   ],
   "source": [
    "from sympy import symbols, expand\n",
    "\n",
    "x = symbols('x')\n",
    "expr = (x + 1)**2\n",
    "expanded_expr = expand(expr)\n",
    "\n",
    "print(\"Original:\", expr)\n",
    "print(\"Expanded:\", expanded_expr)\n"
   ]
  }
 ],
